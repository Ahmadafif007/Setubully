<div class="row text-center p-2">
    <h1>Welcome {{username}}!</h1>
</div>
<div class="row justify-content-center">
<div class="card

          align
          mx-4
          mt-4
          bg-light
          text-black
          customCard "  style="width: 25rem;">
            <img src="https://user-images.githubusercontent.com/82244776/134595680-b39c82e2-0d0d-40bb-a0d6-0a6cd7899227.png" class="card-img-top" alt="profile-pic">
        <h4 class="card-header">Welcome {{username}}</h4>
        <section class="card-body ">
                <div>
                <p>{{first_name}} {{last_name}} ({{nickname}})</p>
            </div>
            <div>
                <p>About me:<br>
                    {{about}}</p>
            </div>
            <div>
                <p>Contact me: {{email}}</p>
            </div>
             <a href="/profile" class="btn btn-primary">Update Profile</a>
        </section>
    </div>
</div>
{{#each posts as |post|}}
{{> dash-posts}}
{{/each}}
