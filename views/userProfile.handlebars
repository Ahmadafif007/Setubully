<div class="row justify-content-center">
<div class="card
          align
          mx-4
          mt-4
          bg-light
          text-black
          customCard "  style="width: 25rem;">
            <img src="https://user-images.githubusercontent.com/82244776/134595680-b39c82e2-0d0d-40bb-a0d6-0a6cd7899227.png" class="card-img-top" alt="profile-pic">
        <h4 class="card-header"></h4>
        <section class="card-body ">
            {{#if isPrivate}}
            <div>
                <p>{{first_name}} {{last_name}}</p>
            </div>
            <div>
                <p>About me:<br>
                    {{about}}</p>
            </div>
            <div>
                {{else}}
                <div>
                <p>{{first_name}} {{last_name}} ({{nickname}})</p>
            </div>
            <div>
                <p>About me:<br>
                    {{about}}</p>
            </div>
            <div>
                <p>Contact me: {{email}}</p>
            </div>
            {{/if}}
        </section>
    </div>
</div>
